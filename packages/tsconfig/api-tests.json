{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./node.json",
  "compilerOptions": {
    "types": ["vitest/globals"],
    "lib": ["ESNext"],
    "target": "ESNext",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noEmit": true
  }
}
